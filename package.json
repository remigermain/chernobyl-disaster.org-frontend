{
  "name": "chernobyl",
  "version": "1.0.0",
  "private": true,
  "author": "rgermain <contact@germainremi.fr>",
  "license": "MIT",
  "scripts": {
    "dev": "nuxt",
    "build": "yarn build:pre && yarn build:nuxt",
    "buildLnpm": "npm run build:pre && npm run build:nuxt",
    "build:nuxt": "nuxt build",
    "build:pre": "./scripts/prebuild.sh",
    "start": "nuxt start",
    "export": "nuxt export",
    "serve": "nuxt serve",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint": "npm run lint:js",
    "clean": "rm -rf .nuxt *.html node_modules/.cache",
    "fclean": "yarn clean && rm -rf node_modules",
    "analyze": "nuxt build --analyze",
    "test": "jest",
    "pretty": "prettier --write \"./**/*.{js,jsx,json}\"",
    "makemessages": "python3 ./scripts/generate-locales.py --compile --default --ignore-error --no-print"
  },
  "dependencies": {
    "@nuxtjs/auth": "^4.9.1",
    "@nuxtjs/axios": "^5.12.0",
    "@nuxtjs/pwa": "^3.1.2",
    "@nuxtjs/robots": "^2.5.0",
    "@nuxtjs/sitemap": "^2.4.0",
    "@nuxtjs/toast": "^3.3.1",
    "luxon": "^1.28.1",
    "nuxt": "^2.15.3",
    "nuxt-i18n": "^6.21.1",
    "sanitize-html": "^2.1.0",
    "v-autosize": "^1.0.3",
    "vue-datetime": "^1.0.0-beta.13",
    "vue-matomo": "^3.14.0-0",
    "vue-multiselect": "^2.1.6",
    "vue-quill-editor": "^3.0.6",
    "vue-toastification": "^1.7.11",
    "vue2-timepicker": "^1.1.5",
    "weekstart": "^1.1.0"
